public List<sObject> getListOfsObject(List<Account> accounts, List<Contact> contacts)
{
    List<sObject> listOfsObject = new List<sObject>();
    if(accounts != null){
        listOfsObject.addAll(accounts);
    }
    if(contacts != null){
        listOfsObject.addAll(contacts);
    }
    return listOfsObject;
}