public List<String> getFieldsValue(Account acc, List<String> fields)
{
    // code here
    List<String> result = new List<String>();
    for(String field : fields){
        if(acc.field != null){
            result.add(String.ValueOf(acc.field));
        }else{
            result.add(null);
        }
    }
    return result;
}