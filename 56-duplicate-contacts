public Boolean duplicateContacts(Contact c1, Contact c2) {
    //code here
    Boolean result;
    if(c1 != null && c2 != null){
        if((c1.Email != null || c1.Phone != null) && (c2.Email != null || c2.Phone != null)){
            if((c1.Email == c2.Email) || (c1.Phone == c2.Phone)){
                result = true;
            }else{
                result = false;
            }
        }
    }
    return result;
}