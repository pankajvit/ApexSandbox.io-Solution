public Boolean duplicateContacts(Contact c1, Contact c2) {
    //code here
        Boolean resultForEmail;
        Boolean resultForPhone;
        if(c1 != null && c2 != null){
            if((c1.Email != null || c1.Phone != null) && (c2.Email != null || c2.Phone != null)){
                if(String.isBlank(c1.Email) && String.isBlank(c2.Email)){
                    if(c1.Email == c2.Email){
                        resultForEmail = true;
                    }else{
                        resultForPhone = false;
                    }
                }
                if(String.isBlank(c1.Phone) && String.isBlank(c2.Phone)){
                    if(c1.Phone == c2.Phone){
                        resultForPhone = true;
                    }else{
                        resultForPhone = false;
                    }
                }
            }
        }
        if(resultForEmail || resultForPhone){
            return true;
        }else{
            return false;
        }
}