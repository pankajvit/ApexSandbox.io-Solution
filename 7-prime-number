public Boolean isPrime(Integer num) {
    //code here
    Boolean result = false;
    if(num > 1){
        Integer i;
        for(i=2;i<num;i++){
            if(math.Mod(num,i)==0){
                break;
            }
        }
        if(i==num-1){
            result = true;
        }
        result = false;
    }
    return result;
}